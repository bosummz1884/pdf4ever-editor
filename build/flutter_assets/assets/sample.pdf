=3D"whitespace-pre! language-dart"><span>import 'dart:io';
import 'package:flutter/services.dart';
import 'package:path_provider/path_provider.dart';
import 'package:pdf/widgets.dart' as pw;
import 'package:pdf/pdf.dart';
import 'package:pdf4ever/editor/models/edited_text_span.dart';

class PDFExportService {
  Future&lt;File&gt; exportEditedPdf({
    required String originalAssetPath,
    required List&lt;EditedTextSpan&gt; edits,
  }) async {
    final doc =3D pw.Document();

    final bytes =3D await rootBundle.load(originalAssetPath);
    final originalPdf =3D pw.Document.fromPdfData(bytes.buffer.asUint8List(=
));

    final pageTheme =3D await _defaultPageTheme();

    doc.addPage(pw.Page(
      pageTheme: pageTheme,
      build: (context) {
        return pw.Stack(
          children: edits.map((e) {
            return pw.Positioned(
              left: e.boundingBox.left,
              top: e.boundingBox.top,
              child: pw.Text(
                e.text,
                style: pw.TextStyle(
                  font: pw.Font.helvetica(), // swap in custom if needed
                  fontSize: e.style.fontSize,
                  fontWeight: _mapWeight(e.style.fontWeight),
                  fontStyle: _mapStyle(e.style.fontStyle),
                  color: PdfColor.fromInt(e.style.color.value),
                ),
              ),
            );
          }).toList(),
        );
      },
    ));

    final output =3D await getTemporaryDirectory();
    final file =3D File('${output.path}/edited_output.pdf');
    await file.writeAsBytes(await doc.save());
    return file;
  }

  Future&lt;pw.PageTheme&gt; _defaultPageTheme() async {
    return pw.PageTheme(
      pageFormat: PdfPageFormat.a4,
    );
  }

  pw.FontWeight _mapWeight(FontWeight weight) {
    switch (weight) {
      case FontWeight.bold:
        return pw.FontWeight.bold;
      default:
        return pw.FontWeight.normal;
    }
  }

  pw.FontStyle _mapStyle(FontStyle style) {
    switch (style) {
      case FontStyle.italic:
        return pw.FontStyle.italic;
      default:
        return pw.FontStyle.normal;
    }
  }
}
